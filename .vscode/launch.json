    "version": "0.2.0",
    "configurations": [
        {
            "name": "HabitTrackerFlutter-main",
            "request": "launch",
            "type": "dart"
        },
        {
            "name": "HabitTrackerFlutter-main (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "HabitTrackerFlutter-main (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        },
        {
            "name": "animation-chrome",
            "request": "launch",
            "type": "dart",
            "deviceId": "chrome",
            "args": ["--no-sound-null-safety"]
        },
        {
            "name": "animation-android",
            "request": "launch",
            "type": "dart",
            "deviceId": "emulator-5554",
            "args": ["--no-sound-null-safety"]
        }

    ],
    "compounds": [
        {
            "name": "Compound",
            "configurations": ["animation-chrome","animation-android"]
        }
    ]
    
}